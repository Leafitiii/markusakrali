{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next",
        "config": {
          "buildCommand": "npm run build",
          "outputDirectory": "dist"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/dist/server.js" }
    ],
    "env": {
      "PAYLOAD_CONFIG_PATH": "dist/payload/payload.config.js"
    }
  }